<body>
    <h1>LISTE DES stocks</h1>
    <ul>
        <% for(var i = 0; i <data.length; i++){%>
            <li>
                <a>name: <%= data[i].name %> type: <%= data[i].type %> Quantity : <%= data[i].quantity %></a>
